{
  "UserAgent": "aws-amplify-cli/2.0",
  "Version": "1.0",
  "auth": {
    "plugins": {
      "awsCognitoAuthPlugin": {
        "UserAgent": "aws-amplify-cli/0.1.0",
        "Version": "0.1.0",
        "IdentityManager": {
          "Default": {}
        },
        "CredentialsProvider": {
          "CognitoIdentity": {
            "Default": {
              "PoolId": "us-east-1:11c5508c-25db-4d40-baf1-6e15da71b02a",
              "Region": "us-east-1"
            }
          }
        },
        "CognitoUserPool": {
          "Default": {
            "PoolId": "us-east-1:11c5508c-25db-4d40-baf1-6e15da71b02a",
            "AppClientId": "<your_app_client_id>",
            "AppClientSecret": "<your_app_client_secret>",
            "Region": "eu-central-1"
          }
        },
        "Auth": {
          "Default": {}
        }
      }
    }
  }
}
